@TechReport{ rp799,
  author =	 "Smaill, A. and Green, I.",
  title =	 "Higher-order Annotated Terms for Proof Search",
  institution =	 DAI,
  year =	 1996,
  note =	 "Also in ``Theorem Proving in Higher Order Logics'',
                  von Wright, J,. ed, Springer, 1996, pp 399--414",
  abstract =	 "A notion of _embedding_ appropriate to higher-order
                  syntax is described. This provides a representation
                  of annotated formulae in terms of the difference
                  between pairs of formulae. We define substitution
                  and unification for such annotated terms. Using this
                  representation of annotated terms, the proof search
                  guidance technique of _rippling_ can be extended to
                  higher-order theorems. We illustrate this with two
                  selected examples using our implementation of these
                  ideas in lambda-Prolog."
}

